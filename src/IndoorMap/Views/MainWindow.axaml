<Window x:Class="IndoorMap.Views.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:view="using:IndoorMap.Views"
        xmlns:const="using:IndoorMap.Const"
        xmlns:avalonia="using:Material.Icons.Avalonia"
        xmlns:controls="clr-namespace:Material.Styles.Controls;assembly=Material.Styles"
        xmlns:vm="using:IndoorMap.ViewModels"
        x:DataType="vm:MainWindowViewModel"
        mc:Ignorable="d"
        Title="{Binding AppName}"
        Width="640"
        Height="710"
        Background="#3b3b3b">
    
    <ContentControl prism:RegionManager.RegionName="{x:Static const:PrismRegions.FlightMissionViewContentRegion}"/>
</Window>
